{
  "name": "collection",
  "description": "mongojs collection",
  "type": "provider",
  "dependencies": [
    "db"
  ],
  "ports": {
    "input": {
      "collection": {
        "type": "string",
        "title": "Collection",
        "description": "The collection name for this database",
        "required": true
      },
      "db": {
        "type": "object",
        "title": "Db Handle",
        "description": "Expects the `db` port of the mongojs.db node",
        "readonly": true,
        "required": true
      }
    },
    "output": {
      "collection": {
        "type": "object",
        "title": "Db Collection handle",
        "readonly": true
      }
    }
  }
}
