        {
          "ns": "mongojs",
          "name": "remove"
        }
      ],
      "links": [
        {
          "source": 0,
          "target": 1,
          "out": "db",
          "in": "db"
        },
        {
          "source": 1,
          "target": 2,
          "out": "collection",
          "in": "collection"
        }
      ],
      "ns": "mongojs"
    }
