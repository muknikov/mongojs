        {
          "ns": "mongojs",
          "name": "db",
          "context": {
            "connectionString": "chix-test"
          }
        },
        {
          "ns": "mongojs",
          "name": "collection",
          "context": {
            "collection": "test"
          }
        },
        {
          "ns": "mongojs",
          "name": "criteria",
          "context": {
