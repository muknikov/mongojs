  "dependencies": {
    "mongojs": "latest"
  },
  "nodes": [
    {
      "name": "collection",
      "description": "mongojs collection",
      "type": "provider",
      "dependencies": [
        "db"
      ],
      "ports": {
        "input": {
          "collection": {
            "type": "string"
          },
          "db": {
            "type": "object"
          }
        },
        "output": {
          "collection": {
            "type": "object"
          }
        }
      },
      "ns": "mongojs"
    },
    {
      "name": "criteria",
      "description": "mongojs criteria",
      "ports": {
        "input": {
          "criteria": {
            "type": "string"
          }
        },
        "output": {
          "criteria": {
            "type": "object"
          }
        }
      },
      "ns": "mongojs"
    },
    {
      "name": "db",
      "description": "mongojs database",
      "type": "provider",
      "require": {
        "mongojs": "latest"
      },
      "ports": {
        "input": {
          "connectionString": {
            "type": "string"
          }
        },
        "output": {
          "db": {
            "type": "object"
          }
        }
      },
      "ns": "mongojs"
    },
    {
      "name": "find",
      "description": "mongojs find",
      "dependencies": [
        "collection"
      ],
      "ports": {
        "input": {
          "collection": {
            "type": "object"
          },
          "criteria": {
            "type": "object"
          }
        },
        "output": {
          "documents": {
            "type": "object"
          }
        }
      },
      "ns": "mongojs"
    },
    {
      "name": "insert",
      "description": "mongojs insert",
      "dependencies": [
        "collection"
      ],
      "ports": {
        "input": {
          "collection": {
            "type": "object"
          },
          "document": {
            "type": "object"
          }
        },
        "output": {
          "success": {
            "type": "boolean"
          }
        }
      },
      "ns": "mongojs"
    },
    {
      "name": "remove",
      "description": "mongojs remove",
      "dependencies": [
        "collection"
      ],
      "ports": {
        "input": {
          "collection": {
            "type": "object"
          },
          "criteria": {
            "type": "object"
          }
        },
        "output": {
          "success": {
            "type": "boolean"
          }
        }
      },
      "ns": "mongojs"
    }
